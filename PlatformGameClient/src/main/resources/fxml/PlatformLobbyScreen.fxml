<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<VBox prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gamegui.controllers.PlatformLobbyScreenController">
    <children>
        <Label layoutX="55.0" layoutY="174.0" text="Lobby">
            <font>
                <Font size="36.0" />
            </font>
        </Label>
      <AnchorPane prefHeight="313.0" prefWidth="600.0">
         <children>
            <Label fx:id="lblJoinedPlayers" alignment="TOP_LEFT" layoutX="32.0" layoutY="34.0" prefHeight="251.0" prefWidth="291.0" text="Joined Players:" />
         </children>
      </AnchorPane>
      <Button fx:id="btnStart" mnemonicParsing="false" onAction="#onStartAttempt" text="Start Game" />
      <Button fx:id="btnBack" mnemonicParsing="false" onAction="#onBackPressed" text="Exit Lobby" />
    </children>
</VBox>
